wget -O /tmp/Oriental-Mekong.m3u "https://raw.githubusercontent.com/exodiver/IPTV/master/Ark/Oriental/Mekong.m3u8"

wget -O /tmp/Oriental-Euphrates.m3u "https://raw.githubusercontent.com/exodiver/IPTV/master/Ark/Oriental/Euphrates%E2%80%8E.m3u8"

wget -O /tmp/NewWorld-Missouri.m3u "https://raw.githubusercontent.com/exodiver/IPTV/master/Ark/New%20World/Missouri.m3u8"

wget -O /tmp/NewWorld-Amazon.m3u "https://raw.githubusercontent.com/exodiver/IPTV/master/Ark/New%20World/Amazon.m3u8"

wget -O /tmp/Hindustan-East.m3u "https://raw.githubusercontent.com/exodiver/IPTV/master/Ark/Hindustan/East.m3u8"

wget -O /tmp/Hindustan-South.m3u "https://raw.githubusercontent.com/exodiver/IPTV/master/Ark/Hindustan/South.m3u8"

wget -O /tmp/Hindustan-West.m3u "https://raw.githubusercontent.com/exodiver/IPTV/master/Ark/Hindustan/West.m3u8"

wget -O /tmp/Europa-Volga.m3u "https://raw.githubusercontent.com/exodiver/IPTV/master/Ark/Europa/Volga.m3u8"

wget -O /tmp/Europa-Thames.m3u "https://raw.githubusercontent.com/exodiver/IPTV/master/Ark/Europa/Thames.m3u8"

wget -O /tmp/Europa-Seine.m3u "https://raw.githubusercontent.com/exodiver/IPTV/master/Ark/Europa/Seine.m3u8"

wget -O /tmp/Zhongguo-Difang.m3u "https://raw.githubusercontent.com/exodiver/IPTV/master/Ark/Zhongguo/Difang.m3u8"

wget -O /tmp/Zhongguo-Yangtze.m3u "https://raw.githubusercontent.com/exodiver/IPTV/master/Ark/Zhongguo/Yangtze.m3u8"

wget -O /tmp/ARK_DRM.m3u "https://raw.githubusercontent.com/exodiver/IPTV/master/Ark/DRM.m3u8"

wget -O /tmp/Prototype-A.m3u "https://raw.githubusercontent.com/exodiver/IPTV/master/Prototype/A.m3u8"

wget -O /tmp/Prototype-Gugong.m3u "https://raw.githubusercontent.com/exodiver/IPTV/master/Prototype/Gugong.m3u8"

wget -O /tmp/Prototype-Istana.m3u "https://raw.githubusercontent.com/exodiver/IPTV/master/Prototype/Istana.m3u8"


sleep 1
rm -rf /tmp/iptvm3upro
exit 0