#!/bin/sh
rm -rf /tmp/*.m3u
rm -rf /tmp//usr/lib/enigma2/python/Plugins/Extensions/m3u_Pro/m3u_ProM3U/*.m3u
rm -rf /etc/enigma2/*.del > /dev/null
rm -rf /etc/enigma2/*.tv_org
rm -rf /etc/enigma2/*.tv_bak
sleep 1

wget -O /tmp/ReMy_XxX.m3u "https://github.com/remytr-org/git_git/raw/master/remy_xxx.m3u"

wget -O /tmp/ReMy_Premium.m3u "https://github.com/remytr-org/git_git/raw/master/remy_premium.m3u"

wget -O /tmp/TURKEY.m3u "https://raw.githubusercontent.com/iptv-org/iptv/master/channels/tr.m3u"

wget -O /tmp/ARABIAN.m3u "https://raw.githubusercontent.com/iptv-org/iptv/master/channels/ae.m3u"

wget -O /tmp/CHINESS.m3u "https://raw.githubusercontent.com/iptv-org/iptv/master/channels/cn.m3u"

wget -O /tmp/GERMAN.m3u "https://raw.githubusercontent.com/iptv-org/iptv/master/channels/de.m3u"

wget -O /tmp/FRENCH.m3u "https://raw.githubusercontent.com/iptv-org/iptv/master/channels/fr.m3u"

wget -O /tmp/GREEK.m3u "https://raw.githubusercontent.com/iptv-org/iptv/master/channels/gr.m3u"

wget -O /tmp/INDIAN.m3u "https://raw.githubusercontent.com/iptv-org/iptv/master/channels/in.m3u"

wget -O /tmp/XX-PORN.m3u "https://raw.githubusercontent.com/iptv-org/iptv/master/channels/int.m3u"

wget -O /tmp/IRANIAN.m3u "https://raw.githubusercontent.com/iptv-org/iptv/master/channels/ir.m3u"

wget -O /tmp/ITALIAN.m3u "https://raw.githubusercontent.com/iptv-org/iptv/master/channels/it.m3u"

wget -O /tmp/ROMANIAN.m3u "https://raw.githubusercontent.com/iptv-org/iptv/master/channels/ro.m3u"

wget -O /tmp/RUSSIAN.m3u "https://raw.githubusercontent.com/iptv-org/iptv/master/channels/ru.m3u"

wget -O /tmp/UNITED_KINGDOM.m3u "https://raw.githubusercontent.com/iptv-org/iptv/master/channels/uk.m3u"

wget -O /tmp/UNITED_AMERICA.m3u "https://raw.githubusercontent.com/iptv-org/iptv/master/channels/us.m3u"

wget -O /tmp/MIX.m3u "https://raw.githubusercontent.com/iptv-org/iptv/master/channels/unsorted.m3u"

sleep 1
rm -rf /tmp/iptvm3upro
exit 0