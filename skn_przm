#!/bin/sh
opkg install enigma2-plugin-extensions-weatherplugin
opkg install enigma2-plugin-systemplugins-weathercomponenthandler
curl https://github.com/remytr-org/git_git/raw/master/enigma2-plugin-skins-remy-rudream-fhd_openatv-7.0_all.ipk -Lko /tmp/enigma2-plugin-skins-remy-rudream-fhd_openatv-7.0_all.ipk
opkg --force-overwrite install /tmp/enigma2-plugin-skins-remy-rudream-fhd_openatv-7.0_all.ipk
opkg install --force-reinstall /tmp/enigma2-plugin-skins-remy-rudream-fhd_openatv-7.0_all.ipk
echo  'YouTube KANALIMA ABONE OLARAK TAKIP EDEBILIRSINIZ'
echo  'https://www.youtube.com/c/ReMyTr'
sleep 2
killall -9 enigma2